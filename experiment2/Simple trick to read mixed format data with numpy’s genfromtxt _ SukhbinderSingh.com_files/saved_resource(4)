( function () {

	// Convert headers to a string because that's what requests.js expects
	function joinHeaders( headerObj ) {
		return _.pairs( headerObj )
			.map( function ( pair ) { return pair.join( '\u003a\u0020' ) } )
			.join( '\u000d\u000a' )
	}

	// The proxy iframe can't use a URL, so we remove everything before the path
	// see jquery.wpcom-proxy-request.js
	function getStringFromUrl( url ) {
		return url
			.replace( wpApiSettings.root, '' )          // remove https://public-api.wordpress.com/
			.replace( _apiEndpointMap, '' )             // remove wp/v2/sites/123456/
	}

	function pathAndQueryFromGetString( get ) {
		return get
			.replace( /\/?wp\/v2\//, '' )
			.split( '?', 2 );
	}

	function requestIsEmbed( request ) {
		return 'embeds/render' === request.path || '/oembed/1.0/proxy' === request.path;
	}

	function getURLFromEmbedRequest( request ) {
		var pairs = request.query.split( '&' );
		for ( var i=0; i < pairs.length ; i++ ) {
			var keyValPair = pairs[i].split( '=' );
			if ( 'url' === keyValPair[0] || 'embed_url' === keyValPair[0] ) {
				return decodeURIComponent( keyValPair[1] );
			}
		}
	}

	function translateEmbedAPIRequest( request ) {
		request.path = 'embeds/render';
		request.query = 'embed_' + request.query + '&force=wpcom&http_envelope=1';
		request.apiNamespace = request.apiNamespace.replace( 'wp/v2', 'rest/v1.1' );
		return request;
	}

	function translateEmedbAPIResponse( response, request ) {
		if ( 'invalid_embed' === response.error ) {
			// we've tried to embed a URL that can't be embedded. Emulate core's fallback link here.
			var link = document.createElement('a');
			var url = getURLFromEmbedRequest( request );
			link.href = url;
			link.innerText = url;
			var response = {
				html: link.outerHTML,
				type: 'rich',
				provider_name: 'Embed',
			};

		}

		if ( response.result ) {
			response.html = response.result;
		}

		return response;
	}

	function requestIsGutenberg( request ) {
		return 0 === request.path.indexOf( '/gutenberg/v1/' );
	}

	function translateGutenbergAPIRequest( request ) {
		request.path = request.path.replace( /^\/gutenberg\/v1\//, '' );
		request.apiNamespace = request.apiNamespace.replace( 'wp/v2', 'gutenberg/v1' );
		return request;
	}

	// Determine if this is part of a fetchAllMiddleware collection of requests.
	// fetchAllMiddleware will change per_page=-1 request to per_page=100 and parse=false
	function fetchAllMiddlewareWillParse( options ) {
		return ( options.parse === false ) && ( options.query && options.query.indexOf( 'per_page=100' ) !== -1 );
	};

	function createFetchResponse( rawResponse, rawHeaders ) {
		var fetchResponse;
		if ( Array.isArray( rawResponse ) ) {
			fetchResponse = rawResponse.slice( 0 );
		}
		else {
			fetchResponse = Object.assign( {}, rawResponse );
		}
		// Create a fetch-like response object to return and resolve
		// We need to return the original response without .json or .headers
		fetchResponse.json = function () { return Promise.resolve( rawResponse ); };
		fetchResponse.headers = {
			get: function ( name ) { return rawHeaders[ name ]; }
		};
		return fetchResponse;
	}

	/**
	 * Tell wp.apiFetch to go through the iFrame proxy
	 */
	function proxyRequest( options ) {
		var request = Object.assign( {}, options );
		var getString;
		if ( options.path ) {
			getString = options.path;
		} else {
			getString = getStringFromUrl( options.url );
		}
		var requestPathAndQuery = pathAndQueryFromGetString( getString );
		request.path = requestPathAndQuery[ 0 ];
		request.query = requestPathAndQuery[ 1 ];

		if ( options.method && [ 'POST', 'PUT', 'DELETE' ].includes( options.method.toUpperCase() ) ) {
			request.method = options.method.toUpperCase();

			if ( typeof options.body === 'undefined' ) {
				options.body = {};
			}

			if ( ( options.body.constructor.name === 'FormData' ) && options.body.has( 'file' ) ) {
				// options.body is a FormData object which we need to convert to a plain old object
				// Due to error below using Chrome and Safari using the wpcom proxy
				// Error: Uncaught DOMException: Failed to execute 'postMessage' on 'Window': FormData object could not be cloned.
				request.data = _.pairs( Array.from( options.body.entries() ) );
				request.formData = [ [ 'file', options.body.get( 'file' ) ] ];
				delete request.body;
			} else {
				request.body = options.data || {};
			}
		} else {
			request.method = options.method || 'GET';
		}

		// apiNamespace keeps the proxy from adding /rest/v1 to the URL it creates
		request.apiNamespace = _apiEndpointMap;
		delete request.url;

		if ( request.path.startsWith( 'rest/v1.1' ) ) {
			// v1.1 APi call, embed previews uses them
			request.apiNamespace = request.apiNamespace.replace( 'wp/v2', 'rest/v1.1' );
			request.path = request.path.replace( 'rest/v1.1/', '' );
		}

		if ( requestIsEmbed( request ) ) {
			request = translateEmbedAPIRequest( request );
		} else if ( requestIsGutenberg( request ) ) {
			request = translateGutenbergAPIRequest( request );
		}

		// Create a fake promise to mimic $.ajax's three args
		var fakePromise = {
			callback: _.noop,
			errorCallback: _.noop,
			resolve: _.noop,
			reject: _.noop,
			then: function ( callback, errCallback ) {
				this.callback = callback;
				if ( errCallback ) {
					this.errorCallback = errCallback;
				}
				return new Promise( function ( resolve, reject ) {
					fakePromise.resolve = resolve;
					fakePromise.reject = reject;
				} )
			}
		}

		jQuery.wpcom_proxy_request( request )
			.then( function ( response, statusCode, headers ) {
				if ( requestIsEmbed( request ) ) {
					response = translateEmedbAPIResponse( response, request );
					if ( getURLFromEmbedRequest( request ) ) {
						// Core returns all embed previews with a status of 200,
						// the only time we don't want to do that is if preview
						// was called without a url.
						statusCode = 200;
					}
				}

				if ( statusCode >= 300 ) { // 200, 201
					fakePromise.reject( fakePromise.errorCallback( response, statusCode, {} ) );
					return;
				}

				// create xhr-like obj
				var xhr = {
					getAllResponseHeaders: function () { return joinHeaders( headers ) },
					getResponseHeader: function ( name ) { return headers[ name ]; },
				}

				// fetchAllMiddleware wants the response to look like a fetch response object, with
				// .json and .headers methods. If fetchAllMiddleware doesn't handle the response,
				// consumers expect a POJO response object from the api-fetch raw middleware
				var payload = response;
				if ( fetchAllMiddlewareWillParse( request ) ) {
					payload = createFetchResponse( response, headers );
				}

				fakePromise.resolve( fakePromise.callback( payload, statusCode, xhr ) );
			} );

		return fakePromise;
	}

	wp.apiFetch.use( function( options ) {
		return wp.apiFetch.fetchAllMiddleware( options, proxyRequest );
	} );

} )();
;
